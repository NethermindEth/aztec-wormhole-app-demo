{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "out/**",
        "bin/**",
        "target/**"
      ]
    },
    "dev": {
      "cache": false
    },
    "test": {
      "dependsOn": [
        "^build"
      ]
    },
    "lint": {},
    "clean": {}
  }
}

